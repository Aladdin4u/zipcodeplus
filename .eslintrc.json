{
    "extends": "airbnb-base",
    "parserOptions": { "ecmaVersion": 2015 },
    "parser": "babel-eslint",
    "rules":{
      "quotes": ["error", "double"],
      "no-console": ["error", { "allow": ["log"] }],
      "func-names": ["error", "never"],
      "import/prefer-default-export": [
        ( "off" | "warn" | "error" ),
      { "target": "single" | "any" } // default is "single"
    ]
    }
  }